{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["standard-with-typescript", "plugin:prettier/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules":  {
        "@typescript-eslint/no-non-null-assertion": "off",
        "@typescript-eslint/no-misused-promises": "off",
        "@typescript-eslint/no-unsafe-assignment": "off",
    },
    "ignorePatterns": ["dist/"]
}